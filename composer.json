{
  "name": "sholomka/image_client",
  "description": "package for the Composer, which will deal with the fact that from the remote host to upload images and save them on the file system.",
  "keywords": [
    "image", "client"
  ],
  "homepage": "https://github.com/sholomka/ComposerPackage",
  "license": "MIT",
  "authors": [
    {
      "name": "Yuriy Sholom",

      "email": "sholomka@gmail.com",

      "homepage": "https://github.com/sholomka",

      "role": "Developer"
    }
  ],
  "require": {
    "php" : ">=7.0.8"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*"
  },
  "autoload": {
    "psr-4": {
      "Sholomka\\Image\\": "src",
      "Sholomka\\Image\\Test\\": "tests"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sholomka\\Image\\Test\\": "tests",
      "Sholomka\\Image\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
